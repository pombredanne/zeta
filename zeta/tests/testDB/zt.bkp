CREATE TABLE `ticket` (
  `id` int(11) NOT NULL auto_increment,
  `ticket_number` int(11) NOT NULL,
  `summary` varchar(128) NOT NULL,
  `description` mediumtext,
  `descriptionhtml` mediumtext,
  `type_id` int(11) NOT NULL,
  `severity_id` int(11) NOT NULL,
  `tsh_id` int(11) default NULL,
  `created_on` datetime default NULL,
  PRIMARY KEY  (`id`),
  KEY `type_id` (`type_id`),
  KEY `severity_id` (`severity_id`),
  CONSTRAINT `ticket_ibfk_1` FOREIGN KEY (`type_id`) REFERENCES `ticket_type` (`id`),
  CONSTRAINT `ticket_ibfk_2` FOREIGN KEY (`severity_id`) REFERENCES `ticket_severity` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=234 DEFAULT CHARSET=latin1;
